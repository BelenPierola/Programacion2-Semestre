#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()

{
 int N,I;
 cout<<"Ingrese el numero:  ";
 cin>>N;
 I=1;
 while(I<=N)
 {
	 cout<<I;
	 I=I+1;
 }   
 getch();
}
